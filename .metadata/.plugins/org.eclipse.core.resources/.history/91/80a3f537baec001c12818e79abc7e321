package ejercicio22;

public class Main {
	public static void main(String[] args) {
		String banda = "\t\t\t\t\t\t";
		
		//1
		System.out.println("\033[1;31;44m"+banda);
		System.out.println("\033[1;31;44m"+banda);
		System.out.println("\033[1;31;44m"+banda);
		System.out.println("\033[1;31;44m"+banda);
		System.out.println("\033[1;31;43m"+banda);
		System.out.println("\033[1;31;43m"+banda);
		System.out.println("\033[1;31;43m"+banda);
		System.out.println("\033[1;31;43m"+banda);
		
		// 5
		System.out.println("\033[1;31;42m"+banda);
		System.out.println("\033[1;31;42m"+banda);
		System.out.println("\033[1;31;42m"+banda);
		System.out.println("\033[1;37;47m"+banda);
		System.out.println("\033[1;37;47m"+banda);
		System.out.println("\033[1;37;47m"+banda);
		System.out.println("\033[1;31;42m"+banda);
		System.out.println("\033[1;31;42m"+banda);
		System.out.println("\033[1;31;42m"+banda);
	}

}
