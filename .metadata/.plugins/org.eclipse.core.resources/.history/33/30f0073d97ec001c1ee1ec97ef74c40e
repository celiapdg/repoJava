package ejercicio11;

public class Main {
	public static void main(String[] args) {
		int num = 0;
		System.out.println(incrementar(num));
		System.out.println(decrementar(num));

	}
	
	public static int incrementar(int numero) {
		return numero++;
	}
	
	public static int decrementar(int numero) {
		return numero-=2;
	}
}
