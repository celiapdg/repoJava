package ejercicio16;

public class Main {
	public static void main(String[] args) {
		byte valor = (byte) Math.round(Math.random());
		boolean ok = false;
		String color = "";
		if (valor == 1) {
			ok = true;
			System.out.println(ok);
		};
		
		if (valor == 0) System.out.println(ok);
		
		if (ok) System.out.println("rojo");
		else System.out.println("negro");
	}
}
